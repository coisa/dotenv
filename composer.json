{
    "name": "coisa/dotenv",
    "description": "Autoload environment variables from a .env file",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Felipe SayÃ£o Lobato Abreu",
            "email": "github@felipeabreu.com.br"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "symfony/dotenv": ">=5.4"
    },
    "minimum-stability": "stable",
    "autoload": {
        "files": [
            "src/autoload.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CoiSA\\Dotenv\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "phpunit --color=always"
    }
}
